ZIndex = 8;
Text = tostring(binds[1][1]):sub(14);
Size = UDim2.new(.45,0,.6,0);
BackgroundTransparency = 1;
TextColor3 = Color3.new(1,1,1);
Position = UDim2.new(0.5,0,0.15,0);
Font = Enum.Font.SourceSansLight;
TextScaled = true;
Parent = window.Content;
})
changeButton.MouseButton1Click:Connect(function()
toggleable = false
local a = UserInputService.InputBegan:wait()
if not IsOnMobile() then
currentButton.Text = "Press A Key"
binds[1][1] = a.KeyCode
KToggleKey = binds[1][1]
currentButton.Text = tostring(binds[1][1]):sub(14);
wait()
Notify({"rbxassetid://17301803826", Color3.fromRGB(0,0,0)}, "Keybind changed", "Your keybind has been changed.")
toggleable = true;
else
KTogglekey = "LeftControl"
Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Error", "Your device is unsupported with keybind\nUse the button on the corner left to toggle krunox")
toggleable = true
end
end)
end)



