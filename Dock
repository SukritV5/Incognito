TextColor3 = Color3.new(1,1,1);
Position = UDim2.new(0.5,0,0.15,0);
Font = Enum.Font.SourceSansLight;
TextScaled = true;
Parent = window.Content;
})

local function urm()
toggleable = false
local asy = UserInputService.InputBegan:wait()
if asy.UserInputType == Enum.UserInputType.Keyboard then
crtbtn.Text = "Press A Key"
barbindd[1][1] = a.KeyCode
ToggleKeyCmd = barbindd[1][1]
crtbtn.Text = tostring(barbindd[1][1]):sub(14);
wait()
Notify({"rbxassetid://17301803826", Color3.fromRGB(0,0,0)}, "Keybind changed", "Your keybind has been changed.")
toggleable = true;
else
Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Error", "Your device is unsupported with keybind\nUse the button on the corner left to toggle krunox")
end
end
changebtn.MouseButton1Down:connect(urm)
end)

blurButton.MouseButton1Down:connect(function()
local window = Window.new({width = 165, height = 100, name='Set Blur', icon='rbxassetid://951352122'})
local blurLabel = create('TextLabel',{
ZIndex = 8;
Text = "Blur Level";
