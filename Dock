-- SAVE FILE

function writefileExploit()
	if writefile then
		return true
	end
end

defaultsettings = {
prefix = ':';
autobackdoorscan = true;
ToggleKeyCmd = 'BackSlash';
KToggleKey = 'LeftControl';
KeepWindowsOpen = false;
MCRED = 0;
MCGREEN = 0;
MCBLUE = 0;
KblurLevel = 25;
LibScripts = {};
KBlur = true;
KBing = true;
KGoogle = false;
KEcosia = false;
FastBoot = false;
SmartLib = false;
KCBar = false;
}

defaults = HttpService:JSONEncode(defaultsettings)
nosaves = false
useFactorySettings = function()
prefix = ':';
autobackdoorscan = true;
ToggleKeyCmd = 'BackSlash';
KToggleKey = 'LeftControl';
KeepWindowsOpen = false;
MCRED = 0;
MCGREEN = 0;
MCBLUE = 0;
KblurLevel = 25;
LibScripts = {};
KBlur = true;
KBing = true;
KGoogle = false;
KEcosia = false;
FastBoot = false;
SmartLib = false;
KCBar = false;
end

local loadedEventData = nil
local jsonAttempts = 0
function saves()
	if writefileExploit() and jsonAttempts < 50 then
		if pcall(function() readfile("KRUNOX.FE") end) then
			if readfile("KRUNOX.FE") ~= nil then
				local success, response = pcall(function()
					local json = HttpService:JSONDecode(readfile("KRUNOX.FE"))
if json.prefix ~= nil then prefix = json.prefix else prefix = ':' end

if json.autobackdoorscan ~= nil then autobackdoorscan = json.autobackdoorscan else autobackdoorscan = true end

if json.ToggleKeyCmd ~= nil then ToggleKeyCmd = json.ToggleKeyCmd else ToggleKeyCmd = 'BackSlash' end

if json.KToggleKey ~= nil then KToggleKey = json.KToggleKey else KToggleKey = 'LeftControl' end


if json.KeepWindowsOpen ~= nil then KeepWindowsOpen = json.KeepWindowsOpen else KeepWindowsOpen = false end

if json.MCRED ~= nil then MCRED = json.MCRED else MCRED = 0 end

if json.MCGREEN ~= nil then MCGREEN = json.MCGREEN else MCGREEN = 0 end

if json.MCBLUE ~= nil then MCBLUE = json.MCBLUE else MCBLUE = 0 end

if json.KblurLevel ~= nil then KblurLevel = json.KblurLevel else KblurLevel = 25 end

if json.LibScripts ~= nil then LibScripts = json.LibScripts else LibScripts = {} end

if json.KBlur ~= nil then KBlur = json.KBlur else KBlur = true end

if json.KBing ~= nil then KBing = json.KBing else KBing = true end

if json.KGoogle ~= nil then KGoogle = json.KGoogle else KGoogle = false end

if json.KEcosia ~= nil then KEcosia = json.KEcosia else KEcosia = false end

if json.FastBoot ~= nil then FastBoot = json.FastBoot else FastBoot = false end

if json.SmartLib ~= nil then SmartLib = json.SmartLib else SmartLib = false end

if json.KCBar ~= nil then KCBar = json.KCBar else KCBar = false end


					
end)
				if not success then
					jsonAttempts = jsonAttempts + 1
					warn("Save Json Error:", response)
					warn("Rewriting Save File")
					writefile("KRUNOX.FE", defaults)
					wait()
					saves()
				end
			else
				writefile("KRUNOX.FE", defaults)
				wait()
				saves()
			end
		else
			writefile("KRUNOX.FE", defaults)
			wait()
			if pcall(function() readfile("KRUNOX.FE") end) then
				saves()
			else
				nosaves = true
				useFactorySettings()

game:GetService("StarterGui"):SetCore("SendNotification", {
	Title = "Krunox";
	Text = "Unable to use writefile?";
	Duration = 5;
})
			end
		end
	else
        if jsonAttempts >= 50 then
            nosaves = true
            useFactorySettings()

game:GetService("StarterGui"):SetCore("SendNotification", {
	Title = "Krunox";
	Text = "Unable to read saved data";
	Duration = 5;
})
        else
            useFactorySettings()
        end
	end
end

saves()

function updatesaves()
	if nosaves == false and writefileExploit() then
		local update = {
prefix = prefix;
autobackdoorscan = autobackdoorscan;
ToggleKeyCmd = ToggleKeyCmd;
KToggleKey = KToggleKey;
KeepWindowsOpen = KeepWindowsOpen;
MCRED = MCRED;
MCGREEN = MCGREEN;
MCBLUE = MCBLUE;
KblurLevel = blurLevel;
LibScripts = LibScripts;
KBlur = Blur;
KBing = Bing;
KGoogle = KGoogle;
KEcosia = Ecosia;
FastBoot = FastBoot;
SmartLib = SmartLib;
KCBar = KCBar;
		}
		writefile("KRUNOX.FE", HttpService:JSONEncode(update))
	end
end


