local searchforit = create('TextButton',{
ZIndex = 8;
Text = "Snipe";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .83;
TextColor3 = Color3.new(1,1,1);
