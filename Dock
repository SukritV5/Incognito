bluebox.FocusLost:connect(function(enter)
if enter then
MCBLUE = tonumber(bluebox.Text)
wait()
refreshwindowcolor()
end
end)

greenbox.FocusLost:connect(function(enter)
if enter then
MCGREEN = tonumber(greenbox.Text)
wait()
refreshwindowcolor()
end
end)


redbox.FocusLost:connect(function(enter)
if enter then
MCRED = tonumber(redbox.Text)
wait()
refreshwindowcolor()
end
end)
-- end of the window whole
end)


-- end of settings
end)


