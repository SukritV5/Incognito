createcheckbox(Fastboot, Color3.new(1,1,1), Color3.new(0,0,0), function(arg)
		FastBoot = arg
	end, FastBoot)  -- function createcheckbox(parent,oncolor,offcolor,respone_function,mode)
	createcheckbox(FathomSmartScreen, Color3.new(1,1,1), Color3.new(0,0,0), function(arg)
		SmartLib = arg
	end, SmartLib)  -- function createcheckbox(parent,oncolor,offcolor,respone_function,mode)
	createcheckbox(Blur, Color3.new(1,1,1), Color3.new(0,0,0), function(arg)
		KBlur = arg
	end, KBlur)

ToggleKey.Text = "Toggle Key: " .. tostring(KToggleKey):sub(14)

local function b()
toggleable = false
ToggleKey.Text = "Press A New Key"
local a = UserInputService.InputBegan:wait()
if a.UserInputType == Enum.UserInputType.Keyboard then
binds[1][1] = a.KeyCode
KToggleKey = binds[1][1]
ToggleKey.Text = "Toggle Key: " .. tostring(binds[1][1]):sub(14);
Notify({"rbxassetid://17301803826", Color3.fromRGB(0,0,0)}, "Keybind changed", "Your keybind has been changed.")
wait()
toggleable = true;
else
b()
end
end
ToggleKey.MouseButton1Up:Connect(b)
end)



