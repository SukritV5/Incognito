local uiduim = Instance.new("UICorner")
uiduim.Parent = KMMenu
uiduim.CornerRadius = UDim.new(10,10)


isopening = false
local button = actualbtn -- or wherever your button is
local count = 0 -- initial count, should always be 0
local threshHold = 2 -- you can set this to even 3 of 4 to get triple and quadriple clicks!
local clickTime = 0.5 -- the time within the clicks should be clicked

function onClick ()
	count += 1 -- add 1 to the number of clicks
	
	if count % threshHold == 0 and isopening == false then -- you reached the threshold
	
KMMenu:TweenPosition(UDim2.new(-0.08,0,0,0),Enum.EasingDirection.Out, Enum.EasingStyle.Quint, 1, true)
isopening = true
MMStart.Visible = false
MMClose.Visible = true
startcircle.Visible = true
actualbtn.Visible = false
selectwhite.Visible = true
end
task.wait(0.91)
if isopening == false then
fopenk()
end
	
	wait(0.20) -- just wait to invalidate the click
	count -= 1 -- invalidate the click
isopening = false
end
button.TouchTap:Connect(onClick) -- yes, it even works for touch screen!


txtexec.FocusLost:Connect(function()
if KCBar then

cmdframe.Visible = true
thearrow.BackgroundTransparency = 1
txtexec.BackgroundTransparency = 1
thearrow.TextTransparency = 0
txtexec.TextTransparency = 0
cmdframe.BackgroundTransparency = 0
txtexec.Text = ""
else
local tweeninging = game:GetService("TweenService"):Create(cmdframe,TweenInfo.new(0.4),{BackgroundTransparency=1})
tweeninging:Play()

local yeyeye = game:GetService("TweenService"):Create(txtexec,TweenInfo.new(0.4),{TextTransparency=1})
yeyeye:Play()


local mimimi = game:GetService("TweenService"):Create(thearrow,TweenInfo.new(0.4),{TextTransparency=1})
mimimi:Play()
txtexec.Text = ""
wait(0.4)
cmdframe.Visible = false
end
end)

kplr = LclPlayer
local modernChat = cloneref(game:GetService("TextChatService")).ChatVersion == Enum.ChatVersion.TextChatService


local Commands = {}
function AddCommand(CmdName, func)
        Commands[CmdName] = CmdName
if modernChat then
TextChatService.SendingMessage:Connect(function(msg)
msg = msg.text:lower()
                args = msg:split(' ')
                if args[1] == prefix..CmdName then
wait()
local success, err = pcall(function()
coroutine.wrap(function()
func()
end)()
end)
                        if success then
                                if args[2] then
-- nothing
                                end
                        end
                        if err then
                                Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Error", "Unable to run command." .. err .. "")
                        end
                end
end)
else -- modern else
        kplr.Chatted:Connect(function(msg)
                msg = msg:lower()
                args = msg:split(' ')
                if args[1] == prefix..CmdName then
                        
                        wait()
                        local success, err = pcall(function()
                                coroutine.wrap(function()
                                        func()
                                end)()
                        end)
                        if success then
                                if args[2] then
                                        
                                
                                        
                                end
                        end
                        if err then
                                Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Error", "Unable to run command." .. err .. "")
                        end
                end
        end)
end
txtexec.FocusLost:Connect(function(text)
                if text then
                        text = txtexec.Text:lower()
                        args = text:split(' ')
                        if args[1] == Commands[CmdName] then

                                wait()
                                local success, err = pcall(function()
                                        coroutine.wrap(function()
                                                func()
                                        end)()
                                end)
                                if success then
                                        if args[2] then
       
                                        
       
                                        end
                                end
                                if err then
                                        Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Error", "Unable to run command." .. err .. "")
                                end
                        end
                end
        end)
end





-- << Audio Controller >> --

function playMusic(id)
local audio = Instance.new("Sound")
audio.Parent = game
audio.Name = "AudioController"
audio.Volume = 10
audio.SoundId = "rbxassetid://"..id
audio.RobloxLocked = true;
audio.Loaded:connect(function()
audio:Play()
end)
return audio
end


function copyclipboard(txt)
    if everyClipboard then
        everyClipboard(tostring(txt))
Notify({"rbxassetid://17345975811", Color3.fromRGB(0,0,0)}, "Copied Successfully", "Text copied to your clipboard.")
    else
Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Unsupported Exploit", "Your Exploit is lacking the Copy to clipboard function.")
    end
end

function refreshwindowcolor()
for i,v in pairs(gui:GetDescendants()) do
if v:IsA("Frame") then
v.BackgroundColor3 = Color3.fromRGB(MCRED,MCGREEN,MCBLUE)
end
if v:IsA("TextButton") then
v.BackgroundColor3 = Color3.fromRGB(MCRED,MCGREEN,MCBLUE)
end
end
end

local refreshCmd = false
function refreshlocalplayer()
local Human = LclPlayer.Character and LclPlayer.Character:FindFirstChildOfClass("Humanoid", true)
	local pos = Human and Human.RootPart and Human.RootPart.CFrame
	local pos1 = workspace.CurrentCamera.CFrame
	LclPlayer.Character.Humanoid.Health = 0
	task.spawn(function()
		LclPlayer.CharacterAdded:Wait():WaitForChild("Humanoid").RootPart.CFrame, workspace.CurrentCamera.CFrame = pos, wait() and pos1
		refreshCmd = false
	end)
end

AddCommand("networkownership", function()
if sethidden then
       local LocalPlayer = game:GetService("Players").LocalPlayer
RunService.Stepped:Connect(function()
settings().Physics.AllowSleep = false
   LocalPlayer.ReplicationFocus = workspace
LocalPlayer.MaximumSimulationRadius = 9e9
sethidden(LocalPlayer, "SimulationRadius", 9e9)
for i,v in pairs(LocalPlayer.Character:GetDescendants()) do
if v:IsA("BasePart") then
sethidden(v, "NetworkIsSleeping", false)
end
end
end)

for i,v in next, LocalPlayer.Character:GetDescendants() do
if v:IsA("BasePart") and v.Name ~="HumanoidRootPart" then 
game:GetService("RunService").Heartbeat:connect(function()
v.Velocity = Vector3.new(-30,0,0)
v.CustomPhysicalProperties = PhysicalProperties.new(0,0,0,0,0)
end)
end
end

LocalPlayer.CharacterAdded:Connect(function()
for i,v in next, LocalPlayer.Character:GetDescendants() do
if v:IsA("BasePart") and v.Name ~="HumanoidRootPart" then
game:GetService("RunService").Heartbeat:connect(function()
v.Velocity = Vector3.new(100,0,0)
end)
end
end
end)
Notify({"rbxassetid://17301803826", Color3.fromRGB(0,0,0)}, "Success", "Net bypass loaded")
while task.wait(8) do
for i,c in pairs(game.workspace:GetDescendants()) do
if c:IsA("BasePart") and c.Name ~="HumanoidRootPart" then
c.CustomPhysicalProperties = PhysicalProperties.new(0,0,0,0,0)
end
end
end
else
Notify({"rbxassetid://17301803826", Color3.fromRGB(0,0,0)}, "Unsupported Exploit", "sethiddenproperty missing.")
end
end)


