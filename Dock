		KBlur = arg
	end, KBlur)

ToggleKey.Text = "Toggle Key: " .. tostring(KToggleKey):sub(14)

local function b()
toggleable = false
ToggleKey.Text = "Press A New Key"
local a = UserInputService.InputBegan:wait()
if a.UserInputType == Enum.UserInputType.Keyboard then
binds[1][1] = a.KeyCode
KToggleKey = binds[1][1]
ToggleKey.Text = "Toggle Key: " .. tostring(binds[1][1]):sub(14);
Notify({"rbxassetid://17301803826", Color3.fromRGB(0,0,0)}, "Keybind changed", "Your keybind has been changed.")
wait()
toggleable = true;
else
b()
end
end
ToggleKey.MouseButton1Up:Connect(b)
end)



keyboardButton.MouseButton1Down:connect(function()

local window = Window.new({width = 165, height = 100, name='Set Key', icon='rbxassetid://950538977'})
local changeButton = create('TextButton',{
ZIndex = 8;
Text = "Set Keybind";
Size = UDim2.new(.45,0,.6,0);
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.new(1,1,1);
Position = UDim2.new(0.05,0,0.15,0);
Font = Enum.Font.SourceSansLight;
TextScaled = true;
Parent = window.Content;
})
local currentButton = create('TextLabel',{
ZIndex = 8;
Text = tostring(binds[1][1]):sub(14);
Size = UDim2.new(.45,0,.6,0);
BackgroundTransparency = 1;
TextColor3 = Color3.new(1,1,1);
Position = UDim2.new(0.5,0,0.15,0);
Font = Enum.Font.SourceSansLight;
TextScaled = true;
Parent = window.Content;
})
changeButton.MouseButton1Click:Connect(function()
toggleable = false
local a = UserInputService.InputBegan:wait()
if not IsOnMobile() then
currentButton.Text = "Press A Key"
binds[1][1] = a.KeyCode
KToggleKey = binds[1][1]
currentButton.Text = tostring(binds[1][1]):sub(14);
wait()
Notify({"rbxassetid://17301803826", Color3.fromRGB(0,0,0)}, "Keybind changed", "Your keybind has been changed.")
toggleable = true;
else
toggleable = true
KTogglekey = "LeftControl"
Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Error", "Your device is unsupported with keybind\nUse the button on the corner left to toggle krunox")
end
end)
end)



